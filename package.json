  "scripts": {
    "build:prod": "npm run clean && npm run build:client:prod && npm run build:server:prod",
    "build:dev": "npm run clean && npm run build:client:dev && npm run build:server:dev",
    "build:client:dev": "webpack --config webpack/webpack.client.config-dev.js --progress --inline --colors",
    "build:client:prod": "webpack --config webpack/webpack.client.config-prod.js --progress --inline --colors",
    "build:server:dev": "webpack --config webpack/webpack.server.config-dev.js --colors",
    "build:server:prod": "webpack --config webpack/webpack.server.config-prod.js --colors",
    "build:watch": "parallelshell 'npm run watch:client:dev' 'npm run watch:server:dev'",
    "clean": "rm -rf build/*",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- --timeout 30000 test/**/*.spec.js",
    "compile": "cross-env NODE_ENV=production npm run babel --presets es2015,stage-0 --ignore node_modules,public -- ./src/server -d ./compiled",
    "start": "parallelshell 'npm run watch:client:dev' 'npm run watch:server:dev' 'cross-env NODE_ENV=development nodemon build/server.bundle.js' ",
    "deploy": "ansible-playbook meeting.yml --ask-become-pass",
    "prod": "npm run build:prod && node build/server.bundle.js",
    "sequelize": "node_modules/.bin/sequelize",
    "test": "cross-env NODE_ENV=test karma start",
    "test:watch": "cross-env NODE_ENV=test npm test -- --watch --no-single-run",
    "test-unit-watch": "mocha --watch --compilers js:babel-core/register test/unit/**/*.spec.js",
    "watch:client:dev": "webpack --config webpack/webpack.client.config-dev.js --progress --inline --colors --watch",
    "watch:server:dev": "webpack --config webpack/webpack.server.config-dev.js --colors --watch",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  }
